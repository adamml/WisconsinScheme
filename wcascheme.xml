<dc:identifier>SDN:I03::003</dc:identifier>
<skos:notation>SDN:I03::003</skos:notation>
<skos:prefLabel xml:lang="en">Bluff Top</skos:prefLabel>
<skos:altLabel/>
<skos:definition xml:lang="en">The top of a high steep bank or cliff.</skos:definition>
<skos:prefLabel xml:lang="fr">Haut de Promontoire</skos:prefLabel>
<skos:definition xml:lang="fr">La partie haute d'un promontoire</skos:definition>
<skos:prefLabel xml:lang="es">Alto del Acantilado</skos:prefLabel>
<skos:definition xml:lang="es">La cima de un despeñadero o acantilado.</skos:definition>
